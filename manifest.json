{
  "manifest_version": 2,
  "name": "SCRAPfy",
  "description": "A collaborative code editor for ninjas.",
  "version": "0.1.3",
  "homepage_url": "http://scrapfy.io",
  "browser_action": {
    "default_title": "SCRAPfy",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "22": "img/icon.png",
    "128": "img/icon-128.png"
  }
}
